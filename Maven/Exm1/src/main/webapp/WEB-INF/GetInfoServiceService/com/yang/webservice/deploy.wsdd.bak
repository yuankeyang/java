<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from GetInfoServiceService WSDL service -->

  <service name="GetInfoService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webservice.yang.com"/>
      <parameter name="wsdlServiceElement" value="GetInfoServiceService"/>
      <parameter name="schemaQualified" value="http://webservice.yang.com"/>
      <parameter name="wsdlServicePort" value="GetInfoService"/>
      <parameter name="className" value="com.yang.webservice.GetInfoServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="GetInfoService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getInfo" qname="operNS:getInfo" xmlns:operNS="http://webservice.yang.com" returnQName="retNS:getInfoReturn" xmlns:retNS="http://webservice.yang.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:sno" xmlns:pns="http://webservice.yang.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getInfo"/>

  </service>
</deployment>
